var d = JSON.parse(jsonData.getValue());
MG.convert.date(d, 'date');
MG.convert.number(d, 'P');

MG.data_graphic({
title: "Confidence Band",
description: "This is an example of a graphic with a confidence band and extended x-axis ticks enabled.",
data: d,
width: 300,
height: 250,
target: ".result",
x_accessor: "date",
y_accessor: "P",
show_secondary_x_label: false,
show_confidence_band: [],
x_extended_ticks: true
});